<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>
    <name>codewizard</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>4.1.0</version>
    <description>childpages.plugin.description</description>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <provider>childpages.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/codewizard/codewizard.png</icon-url>
    <copyright>Copyright 2001-2017 Mairie de Paris</copyright>
    <core-version-dependency>
        <min-core-version>4.0.0</min-core-version>
        <max-core-version/>
    </core-version-dependency>
    <db-pool-required>0</db-pool-required>

    <javascript-files>
        <javascript-files-scope>xpage</javascript-files-scope>
        <javascript-file>codewizard/jquery.metadata.js</javascript-file>
        <javascript-file>codewizard/jquery.tablesorter.js</javascript-file>
        <javascript-file>codewizard/Chart.js</javascript-file>
        <javascript-file>codewizard/custom.js</javascript-file>
        <javascript-file>codewizard/jquery-ui.js</javascript-file>
        <javascript-file>codewizard/prettify/run_prettify.js?skin=desert</javascript-file>
    </javascript-files>

    <!-- Applications -->
    <applications>
        <application>
            <application-id>codewizard</application-id>
            <application-class>fr.paris.lutece.plugins.codewizard.web.CodeWizardApp</application-class>
        </application>
    </applications>
</plug-in>